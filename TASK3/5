#include <stdio.h>
#include <string.h>

int main() {
    char str1[100],str2[100],i;

    printf("Enter the first string:");
    scanf("%s",&str1);
    
    printf("Enter the second string:");
     scanf("%s",&str2);
    
    for( i=0;str1[i]!='\0';i++)
    {
        str1[i] = str2[i];
    }
    str1[i] = '\0';
    str2[i] = '\0';
    printf("The first string is :%s\n",str1);
    printf("The second string is :%s",str2);
}
