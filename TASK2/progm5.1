// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int i,n,a[6];
    printf("Enter the size of array:");
    scanf("%d",&n);
    
    printf("Enter the array elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=n-1;i>=0;i--)
    {
        printf("%d",a[i]);
    }
}
