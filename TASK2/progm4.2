// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int arr[6];
    int size, i, key, found=0;

    printf("Enter size of array: ");
    scanf("%d", &size);

    printf("\nEnter elements in array: ");
    for(i=0; i<size; i++)
    {
        scanf("%d",&arr[i]);
    }

    printf("Enter element to search: ");
    scanf("%d", &key);

 for(i=0; i<size; i++)
    {
        if(arr[i] == key)
        {
            found = 1;
            break;
        }
    }
     if(found == 1)
    {
        printf("\n%d is found at position %d", key, i);
    }
    else
    {
        printf("\n%d is not found in the array", key);
    }
}
